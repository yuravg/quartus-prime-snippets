# -*- mode: snippet -*-
# name: qpp-carry_sum
# key: qpp-Buffers-carry_sum
# group: qpp Buffers
# --
// carry_sum
	carry_sum <instance_name> (
					.sin(<sum_in>),
					.cin(<carry_in>),  //<carry_in> cannot be fed by an input pin
					.sout(<sum_out>),
					.cout(<carry_out>) //<carry_out> cannot feed an output pin
					);
