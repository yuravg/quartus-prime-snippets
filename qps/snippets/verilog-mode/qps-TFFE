# name: qps-TFFE
# key: qps-Registers-and-Latches-TFFE
# group: qps Registers-and-Latches
# --
// TFFE
	TFFE <instance_name> (
				.t(<toggle_signal>),
				.clk(<clock_signal>),
				.clrn(<active_low_clear>),
				.prn(<active_low_preset>),
				.ena(<clock_enable>),
				.q(<data_out>)
				);
