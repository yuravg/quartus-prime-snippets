-- Instantiating CARRY_SUM
	<instance_name> : CARRY_SUM
	-- <carry_in> cannot be fed by an input pin
	-- <carry_out> cannot feed an output pin
	port map(sin => <sum_in>, cin => <carry_in>, sout => <sum_out>, cout => <carry_out>);
